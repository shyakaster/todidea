<!DOCTYPE html>
<html>
<head>
  <title>Todidea</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="flush-message">
  <%if !flash[:notice].blank?%>
      <div class="flash-notice">
        <%= flash[:notice] %>
      </div>
  <%end%>
</div>
<%= yield %>

</body>
</html>